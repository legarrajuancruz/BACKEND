<div class="container">
  <div class="productos">

    <h2>Carrito de compras</h2>
    
    <div class="column">      
        <div id="listadoCarritos">          
          <h3 style=display:none>{{userCart._id}} </h3>         
          </br>
            <div >
                {{#each userCart.products}}
